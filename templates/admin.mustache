<div class="row">
  <div class="columns large-12">
    <h2 class="title">Administration</h2>

    <table>
      <thead>
        <tr>
          <td>ID</td>
          <td>Description</td>
          <td>Email</td>
          <td>Link</td>
          <td>Created</td>
          <td>Completed</td>
          <td>Success</td>
          <td>Delete</td>
        </tr>
      </thead>
      <tbody> 
        {{#get_all}}
          <tr>
            <td>{{id}}</td>
            <td>{{description}}</td>
            <td>{{email}}</td>
            <td><a href="/report/{{id}}">Link</a></td>
            <td>{{created_at}}</td>
            <td>{{completed_at}}</td>
            <td>{{success}}</td>
            <td><a href="/admin/delete/{{delete}}">Delete</a></td>
          </tr>
        {{/get_all}}
      </tbody>
    </table>
  </div>
</div>